        <TextBlock HorizontalAlignment="Left" Margin="20,0,0,0" TextWrapping="Wrap" Text="Заказы" VerticalAlignment="Center" Height="18"/>
        <DataGrid Grid.Row="1">
            <DataGrid.Columns>
                <DataGridTextColumn Header="1" Width="*"/>
                <DataGridTextColumn Header="1" Width="*"/>
                <DataGridTextColumn Header="1" Width="*"/>
                <DataGridTextColumn Header="1" Width="*"/>
                <DataGridTextColumn Header="1" Width="*"/>
            </DataGrid.Columns>
        </DataGrid>


    </Grid>
</Page>
